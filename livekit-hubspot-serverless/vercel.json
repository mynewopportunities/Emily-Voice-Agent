{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/webhooks/livekit",
      "destination": "/api/webhooks/livekit.js"
    },
    {
      "source": "/health",
      "destination": "/api/health.js"
    },
    {
      "source": "/",
      "destination": "/api/index.js"
    }
  ]
}
